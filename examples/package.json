{
  "name": "opanuj-frontend-examples",
  "version": "1.0.0",
  "description": "Examples from Opanuj Frontend course",
  "main": "-",
  "scripts": {
    "example": "node ./scripts/vite.mjs",
    "example:debug": "node ./scripts/vite.mjs --debug",
    "verify": "node ./scripts/verify.mjs",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "vitest run",
    "test:e2e": "npx playwright install && npx playwright test",
    "test:e2e:ui": "npx playwright install && npx playwright test --ui",
    "lint:e2e": "npx eslint **/{tests,pages,mocks}/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/przeprogramowani/opanuj-frontend-praktyka.git"
  },
  "keywords": [
    "frontend",
    "przeprogramowani"
  ],
  "author": "Przeprogramowani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/przeprogramowani/opanuj-frontend-praktyka/issues"
  },
  "homepage": "https://github.com/przeprogramowani/opanuj-frontend-praktyka#readme",
